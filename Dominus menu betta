local uilibiri = {}

local tabsinstance
local continstance
local allinstance
local ids = {}

local function notifylib()
	local notify = Instance.new("Frame")
	local UIListLayout = Instance.new("UIListLayout")

	notify.Name = "notify"
	notify.Parent = allinstance
	notify.AnchorPoint = Vector2.new(0.5, 0.5)
	notify.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	notify.BackgroundTransparency = 1
	notify.BorderColor3 = Color3.fromRGB(0, 0, 0)
	notify.BorderSizePixel = 0
	notify.Position = UDim2.new(0.895, 0, 0.499, 0)
	notify.Size = UDim2.new(0, 244, 0, 721)

	UIListLayout.Parent = notify
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Bottom
	UIListLayout.Padding = UDim.new(0, 5)
end

function uilibiri:createwindow(nome, keybind)
	local ui = Instance.new("ScreenGui")
	allinstance = ui
	local Frame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local name = Instance.new("TextLabel")
	local UICorner_2 = Instance.new("UICorner")
	local top = Instance.new("Frame")
	local top_2 = Instance.new("Frame")
	local tabs = Instance.new("Frame")
	local UIListLayout = Instance.new("UIListLayout")
	local container = Instance.new("Frame")
	local quitbutton = Instance.new("TextButton")
	local imagequit = Instance.new("ImageLabel")

	ui.Name = "ui"
	ui.Parent = game:GetService("CoreGui") or game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	
	notifylib()

	Frame.Parent = ui
	Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0.356702626, 0, 0.260050237, 0)
	Frame.Size = UDim2.new(0, 372, 0, 382)
	Frame.Active = true
	Frame.Draggable = true

	UICorner.Parent = Frame

	name.Name = "name"
	name.Parent = Frame
	name.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	name.BorderColor3 = Color3.fromRGB(0, 0, 0)
	name.BorderSizePixel = 0
	name.Size = UDim2.new(0, 372, 0, 36)
	name.Font = Enum.Font.SourceSansBold
	name.Text = nome
	name.TextColor3 = Color3.fromRGB(255, 255, 255)
	name.TextSize = 16.000
	
	quitbutton.Parent = name
	quitbutton.Text = ""
	quitbutton.BackgroundTransparency = 1
	quitbutton.Size = UDim2.new(0, 20, 0, 20)
	quitbutton.Position = UDim2.new(0.916666687, 0, 0.222222224, 0)
	
	imagequit.Parent = quitbutton
	imagequit.BackgroundTransparency = 1
	imagequit.Size = UDim2.new(0, 20, 0, 20)
	imagequit.Image = "rbxassetid://4458805208"
	imagequit.ImageTransparency = 0.6
	
	quitbutton.MouseButton1Click:Connect(function()
		ui:Destroy()
	end)
	
	quitbutton.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(imagequit, TweenInfo.new(0.2), {ImageTransparency = 0}):Play()
	end)
	
	quitbutton.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(imagequit, TweenInfo.new(0.2), {ImageTransparency = 0.6}):Play()
	end)
	
	game:GetService("UserInputService").InputBegan:Connect(function(codo)
		if codo.KeyCode == Enum.KeyCode[keybind] then
			ui.Enabled = not ui.Enabled
		end
	end)
	
	UICorner_2.Parent = name

	top.Name = "top"
	top.Parent = Frame
	top.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	top.BorderColor3 = Color3.fromRGB(0, 0, 0)
	top.BorderSizePixel = 0
	top.Position = UDim2.new(0, 0, 0.0785340294, 0)
	top.Size = UDim2.new(0, 372, 0, 6)

	top_2.Name = "top"
	top_2.Parent = Frame
	top_2.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	top_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	top_2.BorderSizePixel = 0
	top_2.Position = UDim2.new(0, 0, 0.0942408368, 0)
	top_2.Size = UDim2.new(0, 372, 0, 1)

	tabs.Name = "tabs"
	tabs.Parent = Frame
	tabs.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	tabs.BackgroundTransparency = 1.000
	tabs.BorderColor3 = Color3.fromRGB(0, 0, 0)
	tabs.BorderSizePixel = 0
	tabs.Position = UDim2.new(0.018817205, 0, 0.0994764417, 0)
	tabs.Size = UDim2.new(0, 358, 0, 31)
	
	tabsinstance = tabs

	UIListLayout.Parent = tabs
	UIListLayout.FillDirection = Enum.FillDirection.Horizontal
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.VerticalAlignment = Enum.VerticalAlignment.Center
	UIListLayout.Padding = UDim.new(0, 5)

	container.Name = "container"
	container.Parent = Frame
	container.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	container.BackgroundTransparency = 1.000
	container.BorderColor3 = Color3.fromRGB(0, 0, 0)
	container.BorderSizePixel = 0
	container.Position = UDim2.new(0, 0, 0.215, 0)
	container.Size = UDim2.new(0, 372, 0, 300)
	
	continstance = container
end

function uilibiri:createtab(nome)
	local botao = Instance.new("Frame")
	local botaoclick = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")
	local UICorner_2 = Instance.new("UICorner")
	local redor = Instance.new("UIStroke")
	local tab = Instance.new("Frame")
	local UIListLayout = Instance.new("UIListLayout")

	botao.Name = "botao"
	botao.Parent = tabsinstance
	botao.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	botao.BackgroundTransparency = 1
	botao.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botao.BorderSizePixel = 0
	botao.Size = UDim2.new(0, 60, 0, 20)

	tab.Name = nome
	tab.Parent = continstance
	tab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	tab.BackgroundTransparency = 1.000
	tab.BorderColor3 = Color3.fromRGB(0, 0, 0)
	tab.BorderSizePixel = 0
	tab.Size = UDim2.new(0, 372, 0, 313)
	tab.Visible = false
	
	UIListLayout.Parent = tab
	UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0, 10)
	
	redor.Color = Color3.fromRGB(50, 50, 50)
	redor.Parent = botao
	redor.Transparency = 1
	redor.Name = "clicked"

	botaoclick.Name = "botaoclick"
	botaoclick.Parent = botao
	botaoclick.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	botaoclick.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botaoclick.BorderSizePixel = 0
	botaoclick.Size = UDim2.new(0, 60, 0, 20)
	botaoclick.AutoButtonColor = false
	botaoclick.Font = Enum.Font.SourceSansBold
	botaoclick.Text = nome
	botaoclick.TextColor3 = Color3.fromRGB(255, 255, 255)
	botaoclick.TextSize = 14.000
	
	botaoclick.MouseButton1Click:Connect(function()
		for _, tira in pairs(tabsinstance:GetChildren()) do
			if tira:IsA("Frame") then
			game:GetService("TweenService"):Create(tira.clicked, TweenInfo.new(0.2), {Transparency = 1}):Play()
			end
		end
		for _, tira2 in pairs(continstance:GetChildren()) do
			tira2.Visible = false
		end
		game:GetService("TweenService"):Create(redor, TweenInfo.new(0.2), {Transparency = 0}):Play()
		tab.Visible = true
	end)

	UICorner.CornerRadius = UDim.new(0.899999976, 0)
	UICorner.Parent = botaoclick

	UICorner_2.CornerRadius = UDim.new(0.899999976, 0)
	UICorner_2.Parent = botao
end

function uilibiri:createbutton(nome, tabnome, cod)
	local botao = Instance.new("Frame")
	local botaoclick = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")
	local UICorner_2 = Instance.new("UICorner")
	local redor = Instance.new("UIStroke")

	botao.Name = "botao"
	botao.Parent = continstance[tabnome]
	botao.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	botao.BackgroundTransparency = 1.000
	botao.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botao.BorderSizePixel = 0
	botao.Position = UDim2.new(0.0241935477, 0, 0.0415335447, 0)
	botao.Size = UDim2.new(0, 350, 0, 30)

	botaoclick.Name = "botaoclick"
	botaoclick.Parent = botao
	botaoclick.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	botaoclick.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botaoclick.BorderSizePixel = 0
	botaoclick.Size = UDim2.new(0, 350, 0, 30)
	botaoclick.AutoButtonColor = false
	botaoclick.Font = Enum.Font.SourceSansBold
	botaoclick.Text = nome
	botaoclick.TextColor3 = Color3.fromRGB(255, 255, 255)
	botaoclick.TextSize = 14.000
	botaoclick.TextWrapped = true
	
	botaoclick.MouseButton1Click:Connect(function()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(50, 50, 50)}):Play()
		task.wait(0.1)
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
		cod()
	end)
	
	botaoclick.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
	end)
	
	botaoclick.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
	end)
	
	redor.Color = Color3.fromRGB(50, 50, 50)
	redor.Parent = botao
	
	UICorner.Parent = botaoclick
	UICorner_2.Parent = botao
end

function uilibiri:createtoggle(nome, tabnome, id, cod)
	local value = false
	ids[id] = false
	local toggle = Instance.new("Frame")
	local botaoclick = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")
	local UICorner_2 = Instance.new("UICorner")
	local Frame = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local mover = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local redor = Instance.new("UIStroke")

	toggle.Name = "toggle"
	toggle.Parent = continstance[tabnome]
	toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	toggle.BackgroundTransparency = 1.000
	toggle.BorderColor3 = Color3.fromRGB(0, 0, 0)
	toggle.BorderSizePixel = 0
	toggle.Position = UDim2.new(0.0241935477, 0, 0.0415335447, 0)
	toggle.Size = UDim2.new(0, 350, 0, 30)
	
	redor.Color = Color3.fromRGB(50, 50, 50)
	redor.Parent = toggle

	botaoclick.Name = "botaoclick"
	botaoclick.Parent = toggle
	botaoclick.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	botaoclick.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botaoclick.BorderSizePixel = 0
	botaoclick.Size = UDim2.new(0, 350, 0, 30)
	botaoclick.AutoButtonColor = false
	botaoclick.Font = Enum.Font.SourceSansBold
	botaoclick.Text = nome
	botaoclick.TextColor3 = Color3.fromRGB(255, 255, 255)
	botaoclick.TextSize = 14.000
	botaoclick.TextWrapped = true

	UICorner.Parent = botaoclick
	

	UICorner_2.Parent = toggle

	Frame.Parent = toggle
	Frame.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0.860000014, 0, 0.245000005, 0)
	Frame.Size = UDim2.new(0, 35, 0, 15)

	UICorner_3.Parent = Frame
	UICorner_3.CornerRadius = UDim.new(0.9, 0)

	mover.Name = "mover"
	mover.Parent = Frame
	mover.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
	mover.BorderColor3 = Color3.fromRGB(0, 0, 0)
	mover.BorderSizePixel = 0
	mover.Size = UDim2.new(0, 15, 0, 15)
	
	botaoclick.MouseButton1Click:Connect(function()
		if value then
			value = false
			ids[id] = false
			game:GetService("TweenService"):Create(mover, TweenInfo.new(0.4), {Position = UDim2.new(0, 0, 0, 0)}):Play()
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.4), {BackgroundColor3 = Color3.fromRGB(20, 20, 20)}):Play()
		else
			value = true
			ids[id] = true
			game:GetService("TweenService"):Create(mover, TweenInfo.new(0.4), {Position = UDim2.new(0.571, 0, 0, 0)}):Play()
			game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.4), {BackgroundColor3 = Color3.fromRGB(60, 60, 60)}):Play()
		end
		cod()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(50, 50, 50)}):Play()
		task.wait(0.1)
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.1), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
	end)

	botaoclick.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
	end)

	botaoclick.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
	end)

	UICorner_4.CornerRadius = UDim.new(0.899999976, 0)
	UICorner_4.Parent = mover
end

function uilibiri:createslider(nome, tabname, max, id, codo)
	local pode = false
	local encher = false
	local slider = Instance.new("Frame")
	local botaoclick = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")
	local UICorner_2 = Instance.new("UICorner")
	local slider_2 = Instance.new("Frame")
	local UICorner_3 = Instance.new("UICorner")
	local enche = Instance.new("Frame")
	local UICorner_4 = Instance.new("UICorner")
	local tudo = Instance.new("TextLabel")
	local redor = Instance.new("UIStroke")

	slider.Name = "slider"
	slider.Parent = continstance[tabname]
	slider.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	slider.BorderColor3 = Color3.fromRGB(0, 0, 0)
	slider.BorderSizePixel = 0
	slider.Position = UDim2.new(0.0241935477, 0, 0.0415335447, 0)
	slider.Size = UDim2.new(0, 350, 0, 30)
	
	redor.Color = Color3.fromRGB(50, 50, 50)
	redor.Parent = slider

	botaoclick.Name = "botaoclick"
	botaoclick.Parent = slider
	botaoclick.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	botaoclick.BorderColor3 = Color3.fromRGB(0, 0, 0)
	botaoclick.BorderSizePixel = 0
	botaoclick.Position = UDim2.new(0.029, 0, 0, 0)
	botaoclick.Size = UDim2.new(0, 340, 0, 30)
	botaoclick.AutoButtonColor = false
	botaoclick.Font = Enum.Font.SourceSansBold
	botaoclick.Text = nome
	botaoclick.TextColor3 = Color3.fromRGB(255, 255, 255)
	botaoclick.TextSize = 10
	botaoclick.TextWrapped = true
	botaoclick.TextXAlignment = Enum.TextXAlignment.Left

	UICorner.Parent = botaoclick

	UICorner_2.Parent = slider

	slider_2.Name = "slider"
	slider_2.Parent = slider
	slider_2.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	slider_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	slider_2.BorderSizePixel = 0
	slider_2.Position = UDim2.new(0.159999996, 0, 0.233333334, 0)
	slider_2.Size = UDim2.new(0, 280, 0, 15)

	UICorner_3.Parent = slider_2

	enche.Name = "enche"
	enche.Parent = slider_2
	enche.BackgroundColor3 = Color3.fromRGB(120, 120, 120)
	enche.BorderColor3 = Color3.fromRGB(0, 0, 0)
	enche.BorderSizePixel = 0
	enche.Size = UDim2.new(0, 0, 0, 15)

	UICorner_4.CornerRadius = UDim.new(0, 8)
	UICorner_4.Parent = enche

	tudo.Name = "tudo"
	tudo.Parent = slider_2
	tudo.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	tudo.BackgroundTransparency = 1.000
	tudo.BorderColor3 = Color3.fromRGB(0, 0, 0)
	tudo.BorderSizePixel = 0
	tudo.TextSize = 10
	tudo.Position = UDim2.new(0.142857149, 0, 0, 0)
	tudo.Size = UDim2.new(0, 200, 0, 15)
	tudo.Font = Enum.Font.SourceSansBold
	tudo.Text = "0"
	tudo.TextColor3 = Color3.fromRGB(255, 255, 255)

	botaoclick.MouseEnter:Connect(function()
		game:GetService("TweenService"):Create(slider, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(40, 40, 40)}):Play()
	end)

	botaoclick.MouseLeave:Connect(function()
		game:GetService("TweenService"):Create(slider, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
		game:GetService("TweenService"):Create(botaoclick, TweenInfo.new(0.2), {BackgroundColor3 = Color3.fromRGB(30, 30, 30)}):Play()
	end)
	
	slider_2.MouseEnter:Connect(function()
		pode = true
	end)
	
	slider_2.MouseLeave:Connect(function()
		pode = false
	end)
	
	game:GetService("UserInputService").InputBegan:Connect(function(codo)
		if pode then
		if codo.UserInputType == Enum.UserInputType.MouseButton1 then
			encher = true
		end
		end
	end)
	
	game:GetService("UserInputService").InputEnded:Connect(function(codo)
		if codo.UserInputType == Enum.UserInputType.MouseButton1 then
			encher = false
		end
	end)
	
	game:GetService("RunService").Stepped:Connect(function()
		if encher then
			codo()
			local mouse = game:GetService("Players").LocalPlayer:GetMouse()
			local sliderpos = slider_2.AbsolutePosition.X
			local slidersize = slider_2.AbsoluteSize.X
			
			local porcentagem = math.clamp(mouse.X-sliderpos,0,slidersize) / slidersize
			local valor = math.floor(porcentagem * max)
			ids[id] = valor
			tudo.Text = valor
		    game:GetService("TweenService"):Create(enche, TweenInfo.new(0.1), {Size = UDim2.new(0, math.clamp(mouse.X-sliderpos,0,slidersize), 1, 0)}):Play()
		end
	end)
end

function uilibiri:sendnotify(text, segundo)
	local notifyexample = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local TextLabel = Instance.new("TextLabel")
	local UICorner_2 = Instance.new("UICorner")
	local Frame = Instance.new("Frame")
	local Frame_2 = Instance.new("Frame")
	local TextLabel_2 = Instance.new("TextLabel")

	notifyexample.Parent = allinstance.notify
	notifyexample.BackgroundColor3 = Color3.fromRGB(20, 20, 20)
	notifyexample.BorderColor3 = Color3.fromRGB(0, 0, 0)
	notifyexample.BorderSizePixel = 0
	notifyexample.Position = UDim2.new(0.315573782, 0, 0.0221914016, 0)
	notifyexample.Size = UDim2.new(0, 244, 0, 100)
	notifyexample.BackgroundTransparency = 1

	UICorner.Parent = notifyexample

	TextLabel.Parent = notifyexample
	TextLabel.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	TextLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel.BorderSizePixel = 0
	TextLabel.Size = UDim2.new(0, 244, 0, 18)
	TextLabel.Font = Enum.Font.SourceSansBold
	TextLabel.Text = "Notify"
	TextLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel.TextSize = 14.000
	TextLabel.TextYAlignment = Enum.TextYAlignment.Top
	TextLabel.TextTransparency = 1

	UICorner_2.Parent = TextLabel

	Frame.Parent = notifyexample
	Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0, 0, 0.180000007, 0)
	Frame.Size = UDim2.new(0, 244, 0, 1)
	Frame.BackgroundTransparency = 1

	Frame_2.Parent = notifyexample
	Frame_2.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
	Frame_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame_2.BorderSizePixel = 0
	Frame_2.Position = UDim2.new(0, 0, 0.150000006, 0)
	Frame_2.Size = UDim2.new(0, 244, 0, 3)
	Frame_2.BackgroundTransparency = 1

	TextLabel_2.Parent = notifyexample
	TextLabel_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.BackgroundTransparency = 1.000
	TextLabel_2.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextLabel_2.BorderSizePixel = 0
	TextLabel_2.Position = UDim2.new(0, 0, 0.189999998, 0)
	TextLabel_2.Size = UDim2.new(0, 244, 0, 69)
	TextLabel_2.Font = Enum.Font.SourceSansBold
	TextLabel_2.Text = text
	TextLabel_2.TextColor3 = Color3.fromRGB(255, 255, 255)
	TextLabel_2.TextSize = 14.000
	TextLabel_2.TextTransparency = 1
	
	game:GetService("TweenService"):Create(notifyexample, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(TextLabel, TweenInfo.new(0.2), {TextTransparency = 0}):Play()
	game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(Frame_2, TweenInfo.new(0.2), {BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(TextLabel_2, TweenInfo.new(0.2), {TextTransparency = 0}):Play()
	
	task.wait(segundo)
	
	game:GetService("TweenService"):Create(notifyexample, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(TextLabel, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
	game:GetService("TweenService"):Create(Frame, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(Frame_2, TweenInfo.new(0.2), {BackgroundTransparency = 1}):Play()
	game:GetService("TweenService"):Create(TextLabel_2, TweenInfo.new(0.2), {TextTransparency = 1}):Play()
	
	task.wait(0.2)
	
	notifyexample:Destroy()
end

function uilibiri:returnid(id)
	return ids[id]
end


function uilibiri:destroy()
	allinstance:Destroy()
end

return uilibiri
